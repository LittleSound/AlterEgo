{
  "name": "telegram-ai-bot",
  "version": "1.0.50",
  "module": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "docker:up": "bun run with-env -- docker compose up -d",
    "docker:down": "docker compose down",
    "drizzle": "bun run with-env -- drizzle-kit",
    "drizzle:push": "bun run drizzle push",
    "drizzle:generate": "bun run drizzle generate",
    "drizzle:migrate": "bun run drizzle migrate",
    "with-env": "dotenvx run -f .env -f .env.local --overload --ignore=MISSING_ENV_FILE",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@valibot/to-json-schema": "^1.3.0",
    "@yolk-oss/elysia-env": "^3.0.0",
    "drizzle-orm": "^0.44.5",
    "drizzle-typebox": "^0.3.3",
    "elysia": "latest",
    "grammy": "^1.38.2",
    "marked": "^16.2.1",
    "nanoid": "^5.1.5",
    "pg": "^8.16.3",
    "valibot": "^1.1.0",
    "xsai": "^0.4.0-beta.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.2",
    "@dotenvx/dotenvx": "^1.49.0",
    "@types/pg": "^8.15.5",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.34.0"
  },
  "overrides": {
    "@sinclair/typebox": "0.34.33"
  },
  "patchedDependencies": {
    "@xsai/stream-text@0.4.0-beta.3": "patches/@xsai%2Fstream-text@0.4.0-beta.3.patch"
  }
}
